    "throttle",
    "token-bucket",
    "sliding-window",
    "fixed-window",
    "api-rate-limit",
    "symfony",
    "typescript",
    "async"
  ],
  "homepage": "https://github.com/kzeitar/throttle",
  "license": "MIT",
  "authors": [
    {
      "name": "Khaled Zeitar",
      "email": "khalidzeiter9@gmail.com"
    }
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
